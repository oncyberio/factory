{
  "name": "@oncyber-scenes/contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile && npm run scripts:publish",
    "compile": "hardhat compile",
    "node": "hardhat node",
    "test": "npm run test:coverage",
    "test:unit": "hardhat test --logs",
    "test:coverage": "hardhat coverage",
    "lint": "eslint \"**/*.{js,ts}\" && solhint contracts/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix contracts/**/*.sol",
    "fix:deps": "sed -i.bak 's/skipSupportsInterface: false,/skipSupportsInterface: true,/' ./node_modules/hardhat-deploy/dist/src/helpers.js",
    "deploy:localhost": "hardhat --network localhost deploy",
    "deploy:mumbai": "hardhat --network mumbai deploy",
    "deploy:mainnet": "hardhat --network mainnet deploy",
    "deploy:rinkeby": "hardhat --network rinkeby deploy",
    "deploy:ethereum": "hardhat --network ethereum deploy",
    "flatten": "npm run flatten:CyberDestinationFactoryFacet && npm run flatten:CyberDestinationUtilityFactoryFacet",
    "flatten:CyberDestinationFactoryFacet": "hardhat flatten contracts/Destination/CyberDestinationFactoryFacet.sol > deployments/CyberDestinationFactoryFacet.flatten.sol",
    "flatten:CyberDestinationUtilityFactoryFacet": "hardhat flatten contracts/Destination/CyberDestinationUtilityFactoryFacet.sol > deployments/CyberDestinationUtilityFactoryFacet.flatten.sol"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biconomy/mexa": "^2.0.18",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "4.3.1",
    "@solidstate/contracts": "0.0.23",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.3.4",
    "chai-ethers": "0.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.6",
    "hardhat": "^2.6.2",
    "hardhat-deploy": "^0.9.0",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-preprocessor": "^0.1.4",
    "hardhat-tracer": "^1.0.0-alpha.6",
    "hardhat-typechain": "^0.3.5",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {}
}
